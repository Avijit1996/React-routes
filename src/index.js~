import React, { Component } from 'react';
import ReactDOM from 'react-dom';
class App extends Component{
	constructor(props){
		super(props);
		this.state = {
			fname: '',
			lname: ''
		}
	}
changehandler = (event) => {
	this.setState({
		[event.target.name]:event.target.value
	});
}
	render(){
		return(
				<div>
				<form>
				<?php
				echo("ytftfytg");
				?>
				<input type="text" name="fname" placeholder="First name" onChange={this.changehandler}/>
				<input type="text" name="lname" placeholder="Last name" onChange={this.changehandler}/>
				<input type="button" value="submit" />
				</form>

				</div>
			);
	}

}
 ReactDOM.render(
    <App />,
    document.getElementById('root')
  );
